@article{Basu2013,
  title = {Effects of the {{Dopamine D2 Allosteric Modulator}}, {{PAOPA}}, on the {{Expression}} of {{GRK2}}, {{Arrestin-3}}, {{ERK1}}/2, and on {{Receptor Internalization}}.},
  author = {Basu, Dipannita and Tian, Yuxin and Bhandari, Jayant and Jiang, Jian Ru and Hui, Patricia and Johnson, Rodney L and Mishra, Ram K},
