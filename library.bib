@article{Melendez2015,
  title = {A Novel Multiple-Instance Learning-Based Approach to Computer-Aided Detection of Tuberculosis on Chest {{X-rays}}},
  author = {Melendez, Jaime and Van Ginneken, Bram and Maduskar, Pragnya and Philipsen, Rick H H M and Reither, Klaus and Breuninger, Marianne and Adetifa, Ifedayo M O and Maane, Rahmatulai and Ayles, Helen and S??nchez, Clara I.},
  year = {2015},
