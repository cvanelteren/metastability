@article{Tamura2012,
  title = {A Glass-Coated Tungsten Microelectrode Enclosing Optical Fibers for Optogenetic Exploration in Primate Deep Brain Structures},
  author = {Tamura, Keita and Ohashi, Yohei and Tsubota, Tadashi and Takeuchi, Daigo and Hirabayashi, Toshiyuki and Yaguchi, Masae and Matsuyama, Makoto and Sekine, Takeru and Miyashita, Yasushi},
