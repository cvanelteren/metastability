  keywords = {Animals,Biological,Brain,Brain: physiology,Brain: surgery,control,cortex,development,Dura Mater,Dura Mater: surgery,Electrophysiology,Gene Transfer Techniques,genetics,Haplorhini,Human,Humans,Injections,intrinsic,Laboratory,Light,measurement,methods,model,Neurological,Neurosurgical Procedures,Neurosurgical Procedures: methods,Optics,Optogenetics,Optogenetics: methods,physiology,Primate,primates,Research,Research Support,Support,surgery,therapy,vision,visually},
