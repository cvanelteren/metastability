@article{Ploeger2010,
  title = {The Association between Autism and Errors in Early Embryogenesis: What Is the Causal Mechanism?},
  author = {Ploeger, Annemie and Raijmakers, Maartje E J and {van der Maas}, Han L J and Galis, Frietson},
  year = {2010},
  month = apr,
  journal = {Biological psychiatry},
  volume = {67},
  number = {7},
  pages = {602--7},
  publisher = {{Elsevier Inc.}},
  issn = {1873-2402},
  doi = {10.1016/j.biopsych.2009.10.010},
  abstract = {The association between embryonic errors and the development of autism has been recognized in the literature, but the mechanism underlying this association remains unknown. We propose that pleiotropic effects during a very early and specific stage of embryonic development-early organogenesis-can explain this association. In humans early organogenesis is an embryonic stage, spanning Day 20 to Day 40 after fertilization, which is characterized by intense interactivity among body parts of the embryo. This implies that a single mutation or environmental disturbance affecting development at this stage can have several phenotypic effects (i.e., pleiotropic effects). Disturbances during early organogenesis can lead to many different anomalies, including limb deformities, craniofacial malformations, brain pathology, and anomalies in other organs. We reviewed the literature and found ample evidence for the association between autism and different kinds of physical anomalies, which agrees with the hypothesis that pleiotropic effects are involved in the development of autism. The proposed mechanism integrates findings from a variety of studies on autism, including neurobiological studies and studies on physical anomalies and prenatal influences on neurodevelopmental outcomes. The implication is that the origin of autism can be much earlier in embryologic development than has been frequently reported.},
  pmid = {19932467},
  keywords = {Anticonvulsants,Anticonvulsants: adverse effects,Autistic Disorder,Autistic Disorder: epidemiology,Autistic Disorder: etiology,Autistic Disorder: physiopathology,Epilepsy,Epilepsy: drug therapy,Female,Fetal Development,Fetal Development: physiology,Fetal Diseases,Fetal Diseases: epidemiology,Fetal Diseases: physiopathology,Humans,Pregnancy,Pregnancy Complications,Pregnancy Complications: drug therapy,Pregnancy Complications: epidemiology,Rubella,Rubella: complications}
