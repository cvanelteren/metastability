@article{Coecke2021,
  title = {Kindergarden Quantum Mechanics Graduates (...or How {{I}} Learned to Stop Gluing {{LEGO}} Together and Love the {{ZX-calculus}})},
  author = {Coecke, Bob and Horsman, Dominic and Kissinger, Aleks and Wang, Quanlong},
