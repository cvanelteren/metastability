  keywords = {Algorithms,Bayes Theorem,Biofeedback; Psychology,Brain,Brain: physiology,Brain: physiopathology,Electroencephalography,Electroencephalography: methods,Evoked Potentials,Evoked Potentials: physiology,Feedback,Humans,Magnetoencephalography,Magnetoencephalography: methods,Models; Neurological,Models; Statistical,Neural Networks (Computer),Neurobiology,Neurobiology: methods,Parkinson Disease,Parkinson Disease: physiopathology,Reproducibility of Results,Synaptic Transmission,Synaptic Transmission: physiology}
