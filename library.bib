  author = {Xie, J. and Sreenivasan, S. and Korniss, G. and Zhang, W. and Lim, C. and Szymanski, B. K.},
  year = {2011},
  journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
  volume = {84},
