@article{Ruiz2013,
  title = {Optogenetics through Windows on the Brain in the Nonhuman Primate},
  author = {Ruiz, Octavio and Lustig, Brian R and Nassi, Jonathan J and Cetin, Ali and Reynolds, John H and Albright, Thomas D and Callaway, Edward M and Stoner, Gene R and Roe, Anna W},
