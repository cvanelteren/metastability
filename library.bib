@article{Munoz2018,
  title = {Colloquium: {{Criticality}} and Dynamical Scaling in Living Systems},
  author = {Mu{\~n}oz, Miguel A.},
  year = {2018},
  journal = {Reviews of Modern Physics},
  volume = {90},
  number = {3},
  eprint = {1712.04499},
