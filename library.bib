@article{Hite2001,
  title = {The Evolution of Firm Networks: From Emergence to Early Growth of the Firm},
  shorttitle = {The Evolution of Firm Networks},
  author = {Hite, Julie M. and Hesterly, William S.},
  year = {2001},
  journal = {Strategic Management Journal},
  volume = {22},
  number = {3},
  pages = {275--286},
  issn = {1097-0266},
  doi = {10.1002/smj.156},
  abstract = {This paper addresses whether cohesive networks of socially embedded ties or sparse networks rich in structural holes are more conducive to the success of new firms. We propose that the networks of emerging firms evolve in order to adapt to the firm's changing resource needs and resource challenges. As firms emerge, their networks consist primarily of socially embedded ties drawn from dense, cohesive sets of connections. We label these networks identity based. As firms move into the early growth stage, their networks evolve toward more ties based on a calculation of economic costs and benefits. This shift from identity-based to more calculative networks is manifested in the evolution of the firm networks: (1) from primarily socially embedded ties to a balance of embedded and arm's-length relations; (2) from networks that emphasize cohesion to those that exploit structural holes; and (3) from a more path-dependent to a more intentionally managed network. Thus, this paper suggests that both cohesive and sparse networks are conducive to firm performance when they are aligned with and address firms' evolving resource challenges. Copyright \textcopyright{} 2001 John Wiley \& Sons, Ltd.},
  langid = {english},
  keywords = {early growth,emergence,evolution,networks},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/smj.156},
  file = {/home/casper/Zotero/storage/7A6NNH2U/Hite_Hesterly_2001_The evolution of firm networks.pdf;/home/casper/Zotero/storage/MII5YWJ3/smj.html}
}

