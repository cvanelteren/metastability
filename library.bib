}

@article{Gao2019,
  title = {Computation of the {{Boltzmann}} Entropy of a Landscape: A Review and a Generalization},
  author = {Gao, Peichao and Li, Zhilin},
  year = {2019},
  journal = {Landscape Ecology},
  volume = {34},
  number = {9},
  pages = {2183--2196},
