  keywords = {★,Animals,Biological Clocks,Biological Clocks: physiology,Brain Mapping,Brain Waves,Brain Waves: physiology,Cerebral Cortex,Cerebral Cortex: cytology,Cerebral Cortex: physiology,Cognition,Cognition: physiology,Humans,Models,Nerve Net,Nerve Net: physiology,Neural Pathways,Neural Pathways: physiology,Neurological,Neurons,Neurons: physiology}
