  author = {Samaranch, Lluis and Salegio, Ernesto A and San Sebastian, Waldy and Kells, Adrian P and Foust, Kevin D and Bringas, John R and Lamarre, Clementine and Forsayeth, John and Kaspar, Brian K and Bankiewicz, Krystof S},
