@article{Thompson2004,
  title = {Life and Mind: {{From}} Autopoiesis to Neurophenomenology. {{A}} Tribute to {{Francisco Varela}} 1},
  author = {Thompson, Evan},
  year = {2004},
  journal = {Phenomenology and the Cognitive Sciences},
  volume = {3},
