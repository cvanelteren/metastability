@book{2016,
  title = {Paths, {{Diffusion}}, and {{Navigation}}},
  year = {2016},
  journal = {Fundamentals of Human Imaging Connectomics},
  doi = {10.1016/B978-0-12-407908-3.00007-8},
  abstract = {A key question for connectomics is how topological properties of brain graphs relate to the signaling and information processing functions of nervous systems. Three models of neural information flow are considered in the chapter: shortest path routing, diffusion, and navigation. These models are explained with intuitive metaphors. The efficiency with which information can be communicated under these models is quantified with various measures of network integration. Shortest path routing is the most commonly studied model, and is fundamental to the concept of the brain as a small-world network. Shortest path routing assumes that neurons communicate with well-defined destinations in mind, and that the shortest path to these destinations is known globally. These may be unrealistic assumptions in nervous systems, motivating the study of alternative models of neural information flow that does not mandate centralized, homunculus-like control. Diffusion processes imply that the flow of information is much like the flow of fluids in a network of pipes, which propagates simultaneously along multiple `fronts', without being directed toward a single destination. Communicability is the simplest measure quantifying the efficiency with which information can be communicated under this model. Another model is navigation, where each node forwards information to its closest neighbour in physical distance to the desired destination. Navigation performs particularly well in spatially-embedded, scale-free networks like the brain. We discuss the pros and cons of all three models of neural information flow, and show how they have been applied in order to understand the organization of connectomes at various scales. This chapter also introduces readers to a random walker, the Laplacian matrix and the concept of a network morphospace.},
  isbn = {978-0-12-407908-3},
  file = {/home/casper/Zotero/storage/LVDRXCZV/Unknown - 2016 - Paths, Diffusion, and Navigation(2).pdf}
}

